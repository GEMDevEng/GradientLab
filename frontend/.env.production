# Production environment variables for GradientLab frontend

# API URL (Heroku backend)
REACT_APP_API_URL=https://gradientlab-api.herokuapp.com/api

# WebSocket URL (Heroku backend)
REACT_APP_WS_URL=wss://gradientlab-api.herokuapp.com

# Enable mock authentication (set to 'false' for production)
REACT_APP_USE_MOCK_AUTH=false

# Google Analytics ID (if used)
# REACT_APP_GA_ID=UA-XXXXXXXXX-X
